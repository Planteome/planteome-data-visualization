    "Html": "\r\n<p><span style=\"font-size:8pt\"><span style=\"font-size:xx-small\">&nbsp; &nbsp; &nbsp; &nbsp;Hi, W</span></span><span style=\"font-size:x-small\">hat to change here to make&nbsp;different</span><span style=\"font-size:x-small\">&nbsp;paragraph of &nbsp;bibliography\r\n table?</span></p>\r\n<p><span style=\"font-size:8pt\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n</span><span style=\"font-size:8pt; color:blue\">&lt;</span><span style=\"font-size:8pt; color:#a31515\">xsl</span><span style=\"font-size:8pt; color:blue\">:</span><span style=\"font-size:8pt; color:#a31515\">otherwise</span><span style=\"font-size:8pt; color:blue\">&gt;</span></p>\r\n<p><span style=\"font-size:8pt\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n</span><span style=\"font-size:8pt; color:blue\">&lt;</span><span style=\"font-size:8pt; color:#a31515\">xsl</span><span style=\"font-size:8pt; color:blue\">:</span><span style=\"font-size:8pt; color:#a31515\">call-template</span><span style=\"font-size:8pt\">\r\n</span><span style=\"font-size:8pt; color:red\">name</span><span style=\"font-size:8pt; color:blue\">=</span><span style=\"font-size:8pt\">&quot;</span><span style=\"font-size:8pt; color:blue\">format-bibliography-as-paragraphs</span><span style=\"font-size:8pt\">&quot;</span><span style=\"font-size:8pt; color:blue\">&gt;</span></p>\r\n<p><span style=\"font-size:8pt\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n</span><span style=\"font-size:8pt; color:blue\">&lt;</span><span style=\"font-size:8pt; color:#a31515\">xsl</span><span style=\"font-size:8pt; color:blue\">:</span><span style=\"font-size:8pt; color:#a31515\">with-param</span><span style=\"font-size:8pt\">\r\n</span><span style=\"font-size:8pt; color:red\">name</span><span style=\"font-size:8pt; color:blue\">=</span><span style=\"font-size:8pt\">&quot;</span><span style=\"font-size:8pt; color:blue\">bibNodeSet</span><span style=\"font-size:8pt\">&quot;</span><span style=\"font-size:8pt\">\r\n</span><span style=\"font-size:8pt; color:red\">select</span><span style=\"font-size:8pt; color:blue\">=</span><span style=\"font-size:8pt\">&quot;</span><span style=\"font-size:8pt; color:blue\">msxsl:node-set($extendedBib)</span><span style=\"font-size:8pt\">&quot;</span><span style=\"font-size:8pt; color:blue\">/&gt;</span></p>\r\n<p><span style=\"font-size:8pt\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n</span><span style=\"font-size:8pt; color:blue\">&lt;/</span><span style=\"font-size:8pt; color:#a31515\">xsl</span><span style=\"font-size:8pt; color:blue\">:</span><span style=\"font-size:8pt; color:#a31515\">call-template</span><span style=\"font-size:8pt; color:blue\">&gt;</span></p>\r\n<p><span style=\"font-size:8pt\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n</span><span style=\"font-size:8pt; color:blue\">&lt;/</span><span style=\"font-size:8pt; color:#a31515\">xsl</span><span style=\"font-size:8pt; color:blue\">:</span><span style=\"font-size:8pt; color:#a31515\">otherwise</span><span style=\"font-size:8pt; color:blue\">&gt;</span></p>\r\n<p><span style=\"font-size:8pt\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span>\r\n<span style=\"font-size:8pt; color:blue\">&lt;/</span><span style=\"font-size:8pt; color:#a31515\">xsl</span><span style=\"font-size:8pt; color:blue\">:</span><span style=\"font-size:8pt; color:#a31515\">choose</span><span style=\"font-size:8pt; color:blue\">&gt;</span></p>\r\n",
